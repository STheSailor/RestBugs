{
    "index": "<%=toAbsoluteUrl('/bugs')%>",
    "backlog": "<%=toAbsoluteUrl('/bugs/backlog')%>",
    "working": "<%=toAbsoluteUrl('/bugs/working')%>",
    "qa": "<%=toAbsoluteUrl('/bugs/qa')%>",
    "done": "<%=toAbsoluteUrl('/bugs/done')%>",
    <% if(model && model.length > 0){ %>
    "bugs": [
        <% model.forEach(function(bug, index){ %>{
            "title": "<%= bug.title %>",
            "description": "<%= bug.description %>",
            "assignedTo": "<%= bug.assignedTo %>"
            <% if(bug.status === 'Backlog'){ %> 
            ,"move_working": {
                "href": "<%=toAbsoluteUrl('/bugs/working')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            <% } %>
            <% if(bug.status === 'Working'){ %> 
            ,"move_backlog": {
                "href": "<%=toAbsoluteUrl('/bugs/backlog')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            ,"move_qa": {
                "href": "<%=toAbsoluteUrl('/bugs/qa')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            <% } %>
            <% if(bug.status === 'QA'){ %> 
            ,"move_working": {
                "href": "<%=toAbsoluteUrl('/bugs/working')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            ,"move_done": {
                "href": "<%=toAbsoluteUrl('/bugs/done')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            <% } %>
            <% if(bug.status === 'Done'){ %> 
            ,"move_working": {
                "href": "<%=toAbsoluteUrl('/bugs/working')%>",
                "method": "POST",
                "content-type": "application/x-www-form-urlencoded",
                "parameters": [{"id": "<%=bug._id.toHexString()%>"}, "comments"]
            }
            <% } %>
        }<% if(index !== model.length - 1){ %>,<% } %>
        <% }) %>]
    <% } %>
    <% if(title==='Index'){ %>
    "create_bug": {
        "href": "<%=toAbsoluteUrl('/bugs/backlog')%>",
        "method": "POST",
        "content-type": "application/x-www-form-urlencoded",
        "parameters": ["title", "description"]
    }
    <% } %>
}       